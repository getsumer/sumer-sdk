{
  "name": "sumer-sdk",
  "version": "4.2.3",
  "description": "A package that allows devs to track the errors that may occur in EVM-based Dapps",
  "main": "build/module/index.js",
  "typings": "build/module/index.d.ts",
  "module": "build/module/index.js",
  "files": [
    "./build/"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "NODE_ENV=production npm run build:main && NODE_ENV=production npm run build:module",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "build:watch": "tsc --build -w",
    "dev": "tsc -p tsconfig.module.json -w",
    "test": "jest --colors",
    "lint": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "preversion": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/getsumer/sumer-sdk.git"
  },
  "keywords": [],
  "author": {
    "name": "Wagmi Venture Studio"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getsumer/sumer-sdk/issues"
  },
  "homepage": "https://github.com/getsumer/sumer-sdk#readme",
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "axios": "^1.3.3",
    "babel-jest": "^27.1.5",
    "bowser": "^2.11.0",
    "ethers": "^5.7.2",
    "uuid": "^9.0.0"
  },
  "bundleDependencies": [
    "@ethersproject/abi",
    "@ethersproject/providers",
    "axios",
    "babel-jest",
    "bowser",
    "ethers"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-ts-auto-mock": "^2.1.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^27.1.3",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.3"
  }
}
