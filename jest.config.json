{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "cacheDirectory": ".tmp/jestCache",
  "roots": [
    "<rootDir>/tests"
  ],
  "moduleNameMapper": {
    "axios": "axios/dist/node/axios.cjs"
  },
  "globals": {
    "ts-jest": {
      "tsconfig": "<rootDir>/tsconfig.test.json"
    },
    "__DEBUG_BUILD__": true
  }
}
